{
  "mainEntryPointFilePath": "types/index.d.ts",
  "apiReport": {
    "enabled": false,
    "reportFolder": "<projectFolder>/api/"
  },
  "docModel": {
    "enabled": false,
    "apiJsonFilePath": "<projectFolder>/dist/<unscopedPackageName>.api.json"
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "<projectFolder>/dist/ui.d.ts"
  },
  "messages": {
    "tsdocMessageReporting": {
      "tsdoc-undefined-tag": {
        "logLevel": "none"
      }
    },
    "extractorMessageReporting": {
      "ae-missing-release-tag": {
        "logLevel": "none"
      },
      "ae-forgotten-export": {
        "logLevel": "warning"
      }
    }
  }
}
