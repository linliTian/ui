{
  "name": "@rtkwlf/ui",
  "version": "3.0.1-0",
  "main": "dist/ui.cjs.js",
  "module": "dist/ui.es.js",
  "types": "dist/ui.d.ts",
  "license": "MIT",
  "description": "Arctic Wolf Networks React Component Library",
  "scripts": {
    "check:types": "tsc --noEmit",
    "build:types": "tsc --emitDeclarationOnly",
    "build:ts": "tsc -p ./tsconfig.rollup.json --emitDeclarationOnly --removeComments false && api-extractor run --local",
    "gen": "plop",
    "start": "start-storybook -p 9000 --quiet -s public",
    "storybook:build": "build-storybook -o build/storybook/static -s public",
    "storybook:docs": "build-storybook --docs -o docs -s public",
    "docz:clean": "rimraf .docz && docz clean",
    "docz:dev": "docz dev",
    "docz:build": "npm run docz:clean && docz build",
    "docz:serve": "npm run docz:clean && docz build && docz serve",
    "build": "rm -rf dist/ && rm -rf types/ && npm run rollup && npm run build:ts",
    "prettier:write": "prettier --write '{src,.storybook}/**/*.{js,tsx,ts}'",
    "prettier:check": "prettier --check '{src,.storybook}/**/*.{js,tsx,ts}'",
    "test": "jest",
    "rollup": "rollup -c",
    "test:coverage": "jest --coverage",
    "coverage:open": "open ./coverage/lcov-report/index.html",
    "lint": "eslint '**/*.{ts,tsx}'",
    "lint:fix": "eslint '**/*.{ts,tsx}' --fix"
  },
  "dependencies": {
    "@emotion/react": "11.1.4",
    "@emotion/styled": "11.0.0",
    "@fortawesome/fontawesome-svg-core": "1.2.27",
