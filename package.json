    "@emotion/react": "^11.1.3",
    "@emotion/styled": "^11.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-is": "17.0.1",
    "react-select": "3.1.0",
    "react-windowed-select": "^2.0.4"
